{
  "printWidth": 100,
  "trailingComma": "es5",
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "overrides": [
    {
      "files": "*.sol",
      "options": {
        "semi": true,
        "printWidth": 120,
        "tabWidth": 4
      }
    },
    {
      "files": "*.svg",
      "options": {
        "bracketSameLine": true,
        "parser": "html",
        "printWidth": 10000
      }
    }
  ]
}
